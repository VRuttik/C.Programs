#include<stdio.h>

int main(){

   int i=0, even_sum=0, odd_sum=0;

   for(i=0;i<=20;i++){
      
      if(i%2==0){
         even_sum = even_sum + i;
      }
      else{
         odd_sum = odd_sum + i;
         }
   }

   printf("\n The Sum of 1 to 20 even number is: %d\n",even_sum);
   printf("\n The Sum of 1 to 20 odd number is: %d\n",odd_sum);

   return 0; 
}
