#include<stdio.h>
#include<string.h>

int main(){

   char string1[100], string2[100], string3[100];
   int i=0, j=0;

   printf("\n Enter the first string: ");
   scanf("%s", string1);

   printf("\n Enter the second string: ");
   scanf("%s", string2);

   while (string1[i] != '\0')
   {
      string3[j] = string1[i];
      i++;
      j++;
   }
   i=0;

   while (string2[i] != '\0')
   {
      string3[j] = string2[i];
      i++;
      j++;
   }
   string3[j] = '\0';
   printf("\n The concatenated string is: %s", string3);

   return 0;
   
}
